{
  "name": "kramers.app",
  "version": "1.0.0",
  "description": "Matt Kramers' personal website",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:11ty": "eleventy --serve --input ./src",
    "dev:css": "tailwindcss -i ./src/input.css -o ./src/tailwind.css --watch",
    "build": "run-s build:*",
    "build:css": "tailwindcss -i ./src/input.css -o ./src/tailwind.css",
    "build:11ty": "eleventy --input ./src"
  },
  "author": "Matt Kramers",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3"
  }
}
